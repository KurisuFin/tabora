<h1>Listing operators</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Event</th>
    </tr>
  </thead>

  <tbody>
    <% @operators.each do |operator| %>
      <tr>
        <td><%= link_to operator.user.username, operator.user %></td>
        <td><%= link_to operator.event.name, operator.event %></td>
      </tr>
    <% end %>
  </tbody>
</table>